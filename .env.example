# Database
DATABASE_URL=postgresql://user:password@db:5432/bug_bounty_db

# Integrations (uncomment and set your API keys)
# SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
# SHODAN_API_KEY=your_shodan_api_key
# URLSCAN_API_KEY=your_urlscan_api_key
# VIRUSTOTAL_API_KEY=your_virustotal_api_key
# BUGCROWD_API_KEY=your_bugcrowd_api_key
# JIRA_SERVER=https://yourcompany.atlassian.net
# JIRA_USER=your_jira_email
# JIRA_PASSWORD=your_jira_api_token
# GITHUB_TOKEN=your_github_personal_access_token

# Vulnerability Filtering Configuration (set to "false" to disable filtering)
FILTER_IGNORE_THEORETICAL=true
FILTER_IGNORE_SELF_XSS=true
FILTER_IGNORE_VERSION_DISCLOSURE=true
FILTER_IGNORE_COOKIE_FLAGS=true
FILTER_IGNORE_CLICKJACKING=true
FILTER_IGNORE_CSRF=true
FILTER_IGNORE_OPEN_REDIRECT=true
FILTER_IGNORE_EMAIL_SECURITY=true
FILTER_IGNORE_RATE_LIMITING=true
FILTER_IGNORE_PHYSICAL_ACCESS=true
FILTER_IGNORE_UNVERIFIED_SCANS=true
FILTER_IGNORE_EOL_SYSTEMS=true
FILTER_IGNORE_COMMON_FP=true